INSERT INTO mata_kuliah VALUES('F','Fisika'); 

//untuk tabel daftar tutor
(disini masih ada yg salah untuk constraintnya tapi gapaham salahnya yang mana, tolong cek dong)
CREATE TABLE Tutor(
IDTutor INT NOT NULL,
IDMatkul CHAR NOT NULL,
Nama_Tutor VARCHAR(20) NOT NULL,
Waktu TIME,
CONSTRAINT Tutor_IDMatkul_PK PRIMARY KEY(IDMatkul),
CONSTRAINT Tutor_IDMatkul_FK FOREIGN KEY(IDMatkul) REFERENCES Mata_kuliah(IDmk));

