//untuk tabel daftar matkul

CREATE TABLE Mata_kuliah(
IDmk Char(1) NOT NULL,
Mata_kuliah VARCHAR(20) NOT NULL);

INSERT INTO mata_kuliah VALUES('A','Matematika'); 
